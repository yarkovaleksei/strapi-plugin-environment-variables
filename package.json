    "@strapi/strapi": "^4.17.0",
    "@strapi/typescript-utils": "^4.17.0",
    "@types/koa": "^2.14.0",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.2.18",
    "@types/react-highlight-words": "^0.16.7",
    "@types/react-router-dom": "^5.3.3",
