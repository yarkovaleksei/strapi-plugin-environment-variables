    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "rimraf": "^5.0.5",
    "styled-components": "^5.3.11",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@strapi/strapi": "^4.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "styled-components": "^5.3.11"
  }
