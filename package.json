{
  "name": "strapi-plugin-environment-variables",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Plugin for Strapi CMS, which allows you to view and copy environment variables with which the backend is running",
  "strapi": {
    "name": "environment-variables",
    "displayName": "Environment variables",
    "description": "Allows you to view and copy environment variables with which the backend is running",
    "kind": "plugin"
  },
  "author": {
    "name": "Yarkov Aleksei",
    "url": "https://github.com/yarkovaleksei"
  },
  "maintainers": [
    {
      "name": "Yarkov Aleksei",
      "url": "https://github.com/yarkovaleksei"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yarkovaleksei/strapi-plugin-environment-variables.git"
  },
  "keywords": [
    "strapi",
    "plugin",
    "environment",
    "variables",
    "page"
  ],
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.server.json",
    "lint": "eslint .",
    "clear": "rimraf dist"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
